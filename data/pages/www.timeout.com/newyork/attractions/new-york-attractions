<div class="info-wrapper xs-mb2 xs-mt2" data-tracking-props="{&quot;prop59&quot;:&quot;small_list::2::list item::33::Content-Venue-37393::tile::false&quot;,&quot;eVar59&quot;:&quot;D=c59&quot;,&quot;pev2&quot;:&quot;list item&quot;,&quot;prop60&quot;:&quot;Union Square&quot;,&quot;eVar60&quot;:&quot;D=c60&quot;,&quot;prop61&quot;:&quot;Content-Venue-37393&quot;,&quot;eVar61&quot;:&quot;D=c61&quot;}">
