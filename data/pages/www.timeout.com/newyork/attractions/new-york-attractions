                    <a
                data-tracking="{&quot;eventInfo&quot;:{&quot;eventName&quot;:&quot;list item&quot;,&quot;eventAction&quot;:&quot;contentClick&quot;,&quot;timestamp&quot;:1542651572,&quot;type&quot;:&quot;custom&quot;},&quot;attributes&quot;:{&quot;contentType&quot;:&quot;venue&quot;,&quot;entityId&quot;:&quot;27875&quot;,&quot;contentId&quot;:&quot;Content-Venue-27875&quot;,&quot;contentUrl&quot;:&quot;https:\/\/www.timeout.com\/newyork\/attractions\/empire-state-building-manhattan-ny&quot;,&quot;contentName&quot;:&quot;Empire State Building&quot;,&quot;contentPrimaryCategory&quot;:&quot;attractions (category)&quot;,&quot;subCategory1&quot;:&quot;monuments &amp; memorials&quot;,&quot;contentIsSponsored&quot;:false,&quot;entityZone&quot;:&quot;small_list&quot;,&quot;entityZonePosition&quot;:2,&quot;entityElement&quot;:&quot;list item&quot;,&quot;entityPosition&quot;:1,&quot;entityInteractionTarget&quot;:&quot;tile_image&quot;,&quot;entityIsBookable&quot;:true}}"     data-track-click='{&quot;prop59&quot;:&quot;small_list::2::list item::1::Content-Venue-27875::tile_image::true&quot;,&quot;eVar59&quot;:&quot;D=c59&quot;,&quot;pev2&quot;:&quot;list item&quot;,&quot;prop60&quot;:&quot;Empire State Building&quot;,&quot;eVar60&quot;:&quot;D=c60&quot;,&quot;prop61&quot;:&quot;Content-Venue-27875&quot;,&quot;eVar61&quot;:&quot;D=c61&quot;}'

