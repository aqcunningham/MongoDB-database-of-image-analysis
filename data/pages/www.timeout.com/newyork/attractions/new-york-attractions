                    <a
                data-tracking="{&quot;eventInfo&quot;:{&quot;eventName&quot;:&quot;list item&quot;,&quot;eventAction&quot;:&quot;contentClick&quot;,&quot;timestamp&quot;:1542651572,&quot;type&quot;:&quot;custom&quot;},&quot;attributes&quot;:{&quot;contentType&quot;:&quot;venue&quot;,&quot;entityId&quot;:&quot;60111&quot;,&quot;contentId&quot;:&quot;Content-Venue-60111&quot;,&quot;contentUrl&quot;:&quot;https:\/\/www.timeout.com\/newyork\/attractions\/the-9-11-memorial-and-museum&quot;,&quot;contentName&quot;:&quot;The 9\/11 Memorial and Museum&quot;,&quot;contentPrimaryCategory&quot;:&quot;attractions (category)&quot;,&quot;subCategory1&quot;:&quot;monuments &amp; memorials&quot;,&quot;contentIsSponsored&quot;:false,&quot;entityZone&quot;:&quot;small_list&quot;,&quot;entityZonePosition&quot;:2,&quot;entityElement&quot;:&quot;list item&quot;,&quot;entityPosition&quot;:10,&quot;entityInteractionTarget&quot;:&quot;tile_image&quot;,&quot;entityIsBookable&quot;:true}}"     data-track-click='{&quot;prop59&quot;:&quot;small_list::2::list item::10::Content-Venue-60111::tile_image::true&quot;,&quot;eVar59&quot;:&quot;D=c59&quot;,&quot;pev2&quot;:&quot;list item&quot;,&quot;prop60&quot;:&quot;The 9\/11 Memorial and Museum&quot;,&quot;eVar60&quot;:&quot;D=c60&quot;,&quot;prop61&quot;:&quot;Content-Venue-60111&quot;,&quot;eVar61&quot;:&quot;D=c61&quot;}'

