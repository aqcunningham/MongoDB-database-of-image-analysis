<script>
	function testAdblock() {
		if(typeof document.body == 'undefined') {
	        return;
	    }
	    var testad_id = "sponsorText";
	    var testad = document.createElement("DIV");
	    testad.id = testad_id;
	    testad.style.position = "absolute";
	    testad.style.left = "-999px";
	    testad.appendChild(document.createTextNode("&nbsp;"));
	    document.body.appendChild(testad);
	    setTimeout(
	        function () {
	        	var fullContainer = document.getElementById("full-container");
	            if (testad) {
	                var blocked = (testad.clientHeight == 0);
	                if(typeof PearltreesEnvParams !== 'undefined') {
		                PearltreesEnvParams.hasAdBlock = blocked ? 1 : 0;
		            }
		            if (false && blocked) {
		            	var rightCol = document.getElementById("right-column-region");
		            	if (rightCol) {
		            		rightCol.className = rightCol.className + " soonwithpromo";
		            	}
		            }
		            else if (true && blocked) {
		            	document.body.className = document.body.className + ' noWorld4 hide-rightcol';
		            }
		            document.body.removeChild(testad);		            
	            }
	        }
	    , 300);
	}
	testAdblock();
