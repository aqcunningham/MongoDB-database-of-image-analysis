<script>
	$('.contact-popup').click(function(e){
		e.preventDefault();
		var that = $(this);
		var name = that.attr('data-name');
		var email = that.attr('data-email');
		$('#contact-popup').find('#contact-name').text(name);
		$('#contact-popup').find('input[name="person_email"]').val(email);
		$('#contact-popup').foundation('reveal', 'open')
	})
</script>
