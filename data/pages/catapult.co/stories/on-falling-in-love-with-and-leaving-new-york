<script>
  $active_ad_id = null;
  jQuery(document).ready(function(){
    $('body').on('inview', '.story-ad-container', function(event, isInView, visiblePartX, visiblePartY) {
      if (isInView && visiblePartX == 'both' && visiblePartY == 'both') {
        $active_ad_id = $(this).data('id');
        $(this).addClass('in-focus');
        $.ajax({
          url: '/tracking',
          method: 'POST',
          data: {
            tracking: {
              type: 'Tracking::Adv::View',
              trackable_id: $(this).data('tracking-id'),
              trackable_type: $(this).data('tracking-type')
            }
          }
        })
        try { 
          ga('ec:addPromo', {
            'id': 'PROMO_' + $(this).data('tracking-id'),
            'name': $(this).data('tracking-headline')
          });
          ga('send', 'event', 'Ads', 'View', $(this).data('tracking-label'));
        } catch(e) {}
      }
      if(!isInView) {
        $active_ad_id = null;
        $(this).removeClass('in-focus');
      }
    });
    $(".can_float_share_widget .new_share_widget").sticky({
      topSpacing: 96,
      zIndex: 999
    });
  });
</script>
