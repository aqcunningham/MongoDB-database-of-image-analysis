<script>
  $disable_next_page_load = false;
  $page = 1;
  $(function(){
    $(window).scroll(function() {
      if($(window).scrollTop() + $(window).height() > ($(document).height() - $('#footer').height() - 400)) {
        if(!$disable_next_page_load) {
          $disable_next_page_load = true;
          $page = $page + 1;
          $.ajax({
            url: "/editorial/topics/natives-neighborhoods/stories?page=" + $page,
            success: function(data) {
              if(data != "") {
                $('.stories-list').append(data);
                $('img').lazyload();
                $disable_next_page_load = false;
              }
            }
          })
        }
      }
    });
  })
</script>
