<script type="text/javascript">
    (function($, Etsy) {
        function setYes() {
            Etsy.Logger.info("gzip: setYes");
            $.ajax({
                url: '/api/v2/ajax/performance/gzip/yes',
                dataType: 'json',
                data: {_nnc: Etsy.csrf_nonce},
                type: 'POST',
                timeout: 5000
            });
        }
        function setNo() {
            Etsy.Logger.info("gzip: setNo");
            $.ajax({
                url: '/api/v2/ajax/performance/gzip/no',
                dataType: 'json',
                data: {_nnc: Etsy.csrf_nonce},
                type: 'POST',
                timeout: 5000
            });
        }

        Etsy.Logger.info("gzip: sending test ping");
        $.ajax({
            url: '/api/v2/ajax/performance/gzip',
            dataType: 'json',
            timeout: 5000
        }).done(function(data) {
            Etsy.Logger.info("gzip: test ping done");
            var result = (data && data.results && data.results.success) ? data.results.success : "";
            if (result == "gzipped") {
                setYes();
            } else {
                setNo();
            }
        }).fail(function() {
            Etsy.Logger.info("gzip: test ping fail");
            setNo();
        });
    })(window.jQuery, window.Etsy);
</script>


