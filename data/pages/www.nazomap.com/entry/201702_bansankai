        // AdSenseを挿入する。記事数が挿入箇所より少なければスキップ
        var $target = $('.archive-entry');
        if(index <= $target.length) $target.eq(index).before(elmDiv);
    }

    jQuery(function(){
        // 記事一覧系のページでなければスキップ
        if(!/archive/.test(location.href)) return;

        // 広告IDと挿入箇所（何番目の記事の前に挿入するか）を指定する
        var adSlots = [
            ['7191711516',5],
