<form id="hotSearch" name="hotSearch" action="http://hotels.nyny.com/templates/83893/hotels/list?lang=en&currency=USD&filter.sortedBy=our_rec&filtering=true&destination=Manhattan, New+York,NY" method="get">
<input type="hidden" name="pagename" value="ToSearchResults" />
<input type="hidden" name="lang" value="en" />
<input type="hidden" name="currency" value="USD" />
<input type="hidden" name="destination" value="Manhattan, New+York,NY, United States" />
<input type="hidden" name="filter.sortedBy" value="our_rec" />
<input type="hidden" name="targetId" value="AREA-572b0850-4e3f-469b-87b2-c17ed3ea049b|cities" />
<input type="hidden" name="filtering" value="true" />
<input type="hidden" name="requestVersion" value="V2" />

<input type="hidden" name="standardCheckin" value="11/21/2018" />
<input type="hidden" name="standardCheckout" value="11/23/2018" />
<input type="hidden" name="roomsCount" value="1" />
<input type="hidden" name="rooms[0].adultsCount" value="2" />
<input type="hidden" name="rooms[0].childrenCount" value="0" />
</form>
<script type="text/javascript">
$(function(){
	$('.guarantee a').bind('click', function(e){
		e.preventDefault();
		$('#guarantee-modal').modal('show');
	});
	
	$('.modal-close, .modal .close').bind('click', function(){
		$('#guarantee-modal').modal('hide');
	});
	
	$('#book_hotel .checkin').datepicker({
		minDate: "today",
		defaultDate: "+2d",
		dateFormat:"mm/dd/yy",
		onSelect: function(selectedDate){
			$('#book_hotel .checkout').datepicker( "option", "minDate", selectedDate );
			$('input[name="standardCheckin"]').val(selectedDate);
		},
		onClose: function(){
			$('#book_hotel .checkout').datepicker("show");			
		}
	});
	$('#book_hotel .checkout').datepicker({
		defaultDate: "+3d",
		dateFormat:"mm/dd/yy",
		onSelect: function(selectedDate){
			$('#book_hotel .checkin').datepicker('option', 'maxDate', selectedDate);
			$('input[name="standardCheckout"]').val(selectedDate);
		}
	});
	
	//get all room rows
	var rows = $('table.roomSelect tr.roomCount');
	var roomCount = 1;
	//on click get room value
	$('select[name="roomsCount"]').change(function(){
		//get roomCount
		roomCount = $(this).val();
		//remove all classes
		$(rows).removeClass('show');
		
		// enable adults and children dropdown
		$('select#numAdult-rm0').removeAttr('disabled');
		$('select#numChild-rm0').removeAttr('disabled');
		
		//remove hidden input(s) for number of occupants in extra rooms
		$('.extraRoom-adult-total').remove();
		$('.extraRoom-child-total').remove();
		// remove extra hidden input(s) for child's age
		var maxRoomsAvail = $('select#numRooms').length;
		for (n = roomCount; n < maxRoomsAvail; n++){
			$('input[name="rooms['+ n-1 +'].children[0].age"]').remove();
		}
		
		if (roomCount != '8+'){
			for(i = 1; i < roomCount; i++){
				//add them back in
				$(rows[i-1]).addClass('show');
				
				//create hidden input(s) for number of occupants in extra rooms
				$('#hotSearch').append('<input type="hidden" name="rooms[' + i + '].adultsCount" class="extraRoom-adult-total" value="2" />');
				$('#hotSearch').append('<input type="hidden" name="rooms[' + i + '].childrenCount" class="extraRoom-child-total" value="0" />');
			}
			return roomCount;
			
		} else {
			// disable adults and children dropdown
			$('select#numAdult-rm0').attr('disabled', 'disabled');
			$('select#numChild-rm0').attr('disabled', 'disabled');
		}
		
	});
	
	//on change set the number of children
	$('select.room-numChild').change(function(){
		// get number of children in room
		var childCount = $(this).val();
		
		var roomNumPrefix = $(this).attr('name').replace('].childrenCount', '');
		var roomNum = roomNumPrefix.replace('rooms[', '');
		
		// get total number of options for child <select>
		var numChildAvail = $('#numChild-rm' + roomNum + ' option').length;
		
		// cycle through the hidden input(s) for child's age and remove it
		for (x = 0; x < numChildAvail; x++){
			// if hidden input exists, remove it
			if ($('input[name="'+ roomNumPrefix +'].children['+ x +'].age"]').length){
				$('input[name="'+ roomNumPrefix +'].children['+ x +'].age"]').remove();
			}
		}
		
		// create the hidden input(s) for the child's age
		for (i = 0; i < childCount; i++){
			$('#hotSearch').append('<input type="hidden" name="'+ roomNumPrefix +'].children['+ i +'].age" value="0" class="child-age" />');
		}
		// set number of children
		$('input[name="rooms['+ roomNum +'].childrenCount"]').val(childCount);
	});
	
	
	//on change set the number of adults
	$('select.room-numAdult').change(function(){
		// get number of adult(s) in room
		var adultCount = $(this).val();
		
		var roomNumPrefix = $(this).attr('name').replace('].adultsCount', '');
		var roomNum = roomNumPrefix.replace('rooms[', '');
		// set number of adults
		$('input[name="rooms['+ roomNum +'].adultCount"]').val(adultCount);
	});

	$('#book_hotel form').submit(function(e){
		e.preventDefault();
		
		var checkin = $('input[name="checkin"]').val().replace(/^[0]+/g,"").split('/');
		var checkout = $('input[name="checkout"]').val().replace(/^[0]+/g,"").split('/');

		$('input[name="arrivalMonth"]').val(checkin[0] -1);
		$('input[name="departureMonth"]').val(checkout[0] -1);
		$('input[name="arrivalDay"]').val(checkin[1]);
		$('input[name="departureDay"]').val(checkout[1]);
		
		$('input[name="roomsCount"]').val($('select#numRooms').val());
		//$('input[name="room-0-adult-total"]').val($('select#numAdult-rm0').val());
		//$('input[name="room-0-child-total"]').val($('select#numChild-rm0').val());

		var numRoomsSelected = $('select#numRooms').val();

		for ( i = 0; i < numRoomsSelected; i++ ){
			if ( $('input[name="rooms['+ i +'].adultsCount"]').length > 0 ){
				// set the values for hidden input elements
				$('input[name="rooms['+ i +'].adultsCount"]').val($('select#numAdult-rm' + i).val());
				$('input[name="rooms['+ i +'].childrenCount"]').val($('select#numChild-rm' + i).val());

			} else {
				// create hidden input for number of adults & children in room
				$('#hotSearch').append('<input type="hidden" name="rooms['+ i +'].adultsCount" value="' + $('select#numAdult-rm' + i).val() + '" class="extraRoom-adult-total" />');
				$('#hotSearch').append('<input type="hidden" name="rooms['+ i +'].childrenCount" value="' + $('select#numChild-rm' + i).val() + '" class="extraRoom-child-total" />');
			}
			
			var childTotal = $('select#numChild-rm' + i).val();
			
			if (childTotal > 0){
				// hidden inputs for child's age
				for ( n = 0; n < childTotal; n++ ){
					if ( $('input[name="rooms['+ i +'].children['+ n +'].age"]').length == 0 ){
						// create hidden input for child's age if it does not exist
						$('#hotSearch').append('<input type="hidden" name="rooms['+ i +'].children['+ n +'].age" value="0" class="child-age" />');
					} else {
						$('input[name="rooms['+ i +'].children['+ n +'].age"]').val(0);
					}
				}
			}
		}
		
		BLVDS.trackEvent('hotel', 'Find Hotel', 'http://travel.ian.com/index.jsp');
		
		// send form to meetings affiliate if user selects 8+ rooms
		if ($('input[name="roomsCount"]').val() == '8+'){
			var meetingsFormAction = 'http://meetings.nyny.com/?checkIn='+ $('input[name="checkin"]').val() +'&checkOut='+ $('input[name="checkout"]').val();
			$('#hotSearch').attr('action', meetingsFormAction);
		}
		
		$('#hotSearch').submit();
		
		return false;
		
		var url = $(this).attr('action');
		url += '&checkin=' + $('input[name="checkin"]').val();
		url += '&checkout=' + $('input[name="checkout"]').val();
		BLVDS.trackEvent('hotel', 'Find Hotel', url);
      
		window.location = encodeURI(url);
	});
	
});
</script>
</div>
